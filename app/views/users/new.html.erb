<h1> Make an Account Here: </h1>
<% if @user.errors.any? %>
  <% @user.errors.full_messages.each do |message|%>
     <h3><%= message %> </h3>
  <%end%>
<%end%>

<%= form_with model: @user do |f| %>
    <%= f.label :email %> 
    <%= f.text_field :email %>
    <br></br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br></br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Create Your Account' %>
<% end %> 
<br>
<h3> Login with Google:</h3>
<%= button_to 'Google', '/auth/google_oauth2' %>
<%= link_to "Login Here:", login_path %> 