<h1> Make an Account Here: </h1>

<%= form_with model: @user do |f| %>
    <%= f.label :email %> 
    <%= f.text_field :email %>
    <br></br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br></br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Create Your Account' %>
<% end %> 