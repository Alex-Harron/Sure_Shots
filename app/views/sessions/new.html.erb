<h1> Login Here: </h1>

<% @errors && @errors.each do |e|%>
    <%= e %>
<%end%>
<br></br>
<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Log in' %>
<% end %>
<h3> Login with Google:</h3>
<%= button_to "Log in with Google", '/auth/google_oauth2'  %>

<br></br>
<%= link_to "Create Account", signup_path %> 


